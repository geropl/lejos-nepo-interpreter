<?xml version="1.0" encoding="UTF-8"?>
<blockSet>
  <instance>
    <block type="robControls_start" id="start">
      <next>
        <block type="robControls_repeat" id="loop1">
          <field name="TIMES">5</field>
          <statement name="DO">
            <block type="robSensors_touch_isPressed" id="touch1">
              <field name="SENSORPORT">S1</field>
              <next>
                <block type="robControls_if" id="if1">
                  <value name="IF0">
                    <block type="robSensors_touch_isPressed" id="touch_check">
                      <field name="SENSORPORT">S1</field>
                    </block>
                  </value>
                  <statement name="DO0">
                    <block type="robActions_play_tone" id="beep">
                      <field name="FREQUENCY">1000</field>
                      <field name="DURATION">200</field>
                    </block>
                  </statement>
                  <next>
                    <block type="robControls_wait_time" id="wait_loop">
                      <field name="TIME">100</field>
                    </block>
                  </next>
                </block>
              </next>
            </block>
          </statement>
        </block>
      </next>
    </block>
  </instance>
</blockSet>
