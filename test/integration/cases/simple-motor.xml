<?xml version="1.0" encoding="UTF-8"?>
<export xmlns="http://de.fhg.iais.roberta.blockly">
    <program>
        <block_set xmlns="http://de.fhg.iais.roberta.blockly" robottype="nxt" xmlversion="3.1">
            <instance x="384" y="50">
                <block type="robControls_start" id="start_1" intask="true" deletable="false">
                    <statement name="ST">
                        <block type="robActions_motorDiff_on" id="motor_1" intask="true">
                            <field name="DIRECTION">FOREWARD</field>
                            <value name="POWER">
                                <block type="math_number" id="power_1" intask="true">
                                    <field name="NUM">50</field>
                                </block>
                            </value>
                        </block>
                    </statement>
                </block>
            </instance>
        </block_set>
    </program>
    <config>
        <block_set xmlns="http://de.fhg.iais.roberta.blockly" robottype="nxt" xmlversion="3.1">
            <instance x="470" y="50">
                <block type="robBrick_EV3-Brick" id="brick_1" intask="true" deletable="false">
                    <value name="MA">
                        <block type="robBrick_motor_big" id="motor_a" intask="true">
                            <field name="MOTOR_REGULATION">TRUE</field>
                            <field name="MOTOR_REVERSE">OFF</field>
                            <field name="MOTOR_DRIVE">RIGHT</field>
                        </block>
                    </value>
                    <value name="MC">
                        <block type="robBrick_motor_big" id="motor_c" intask="true">
                            <field name="MOTOR_REGULATION">TRUE</field>
                            <field name="MOTOR_REVERSE">OFF</field>
                            <field name="MOTOR_DRIVE">LEFT</field>
                        </block>
                    </value>
                </block>
            </instance>
        </block_set>
    </config>
</export>